{
  "name": "dietfront",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:mock": "vite --mode development",
    "dev:mongo": "vite --mode production",
    "build": "vite build",
    "build:mock": "vite build --mode development",
    "build:mongo": "vite build --mode production",
    "build-deploy": "yarn build",
    "deploy:mock": "yarn build:mock",
    "deploy:mongo": "yarn build:mongo",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage --passWithNoTests",
    "test:ui": "vitest --ui",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "lint": "eslint .",
    "format": "prettier --write .",
    "sass": "sass --watch src/sass/main.scss src/css/appStyle.css",
    "sass:build": "sass src/sass/main.scss src/css/appStyle.css",
    "doc:md": "typedoc --options typedoc-markdown.json",
    "doc:html": "typedoc"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/playfair-display": "^5.1.1",
    "@mui/icons-material": "^6.1.9",
    "@mui/material": "^6.4.8",
    "@mui/styles": "^6.4.8",
    "firebase": "^11.4.0",
    "firebase-tools": "^13.32.0",
    "framer-motion": "^12.4.10",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-google-recaptcha": "^3.1.0",
    "react-icons": "^5.5.0",
    "react-onesignal": "^3.2.2",
    "react-router-dom": "^7.0.1",
    "react-youtube": "^10.1.0",
    "recharts": "^2.15.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-google-recaptcha": "^2.1.9",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.6",
    "c8": "^10.1.3",
    "cypress": "^13.16.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "jsdom": "^25.0.1",
    "msw": "^2.6.6",
    "prettier": "^3.4.1",
    "sass": "^1.81.0",
    "typedoc": "^0.28.5",
    "typedoc-plugin-markdown": "^4.7.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.16.0",
    "vite": "^6.0.1",
    "vitest": "^3.2.4"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}